<body class="template-{{ template | split: '.' | first }}">
  <div class="youtube-wrapper"><a href="#" class="close_btn">X</a><div class="youtube-content"></div></div>
  <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>

  <div id="SearchDrawer" class="search-bar drawer drawer--top">
    <div class="search-bar__table">
      <div class="search-bar__table-cell search-bar__form-wrapper">
        <form class="search search-bar__form" action="/search" method="get" role="search">
          <button class="search-bar__submit search__submit btn--link" type="submit">
            {% include 'icon-search' %}
            <span class="icon__fallback-text">{{ 'general.search.submit' | t }}</span>
          </button>
          <input class="search__input search-bar__input" type="search" name="q" value="{{ search.terms | escape }}" placeholder="{{ 'general.search.placeholder' | t }}" aria-label="{{ 'general.search.placeholder' | t }}">
        </form>
      </div>
      <div class="search-bar__table-cell text-right">
        <button type="button" class="btn--link search-bar__close js-drawer-close">
          {% include 'icon-close' %}
          <span class="icon__fallback-text">{{ 'general.search.close' | t }}</span>
        </button>
      </div>
    </div>
  </div>
  
  {% section 'mobile-nav' %}
  <div class="page">
  {% section 'header' %}
  <div class="page-container" id="PageContainer">

    <main class="main-content" id="MainContent" role="main">
      {{ content_for_layout }}
    </main>

    {% section 'footer' %}

  </div>
  </div>
<script> 
$(document).ready(function(){ 
  $('.faq-link').click(function(ev){  
 ev.preventDefault();
  ev.stopPropagation();
  
  if ($(this).hasClass('selected')==true){
    $(this).removeClass('selected');
    $(this).closest('li').find('.answer').slideUp();
  }else{
    $(this).addClass('selected');
    $(this).closest('li').find('.answer').slideDown();  
  }
  
});
  });
</script>
  <!-- All External Links Open in new Tab -->
  <script>
jQuery('a[href^="http"]').not('a[href^="{{ shop.url }}"]').attr('target', '_blank');
</script>
  <script>
  olark('api.box.hide');
  olark('api.chat.onOperatorsAvailable', function() {
    document.getElementById('header-chat').style.position = 'relative';
    document.getElementById('header-chat-link').style.position = 'relative';
    document.getElementById('header-chat').style.left = '0';
    document.getElementById('header-chat-link').style.left = '0';
    document.getElementById('header-chat').style.opacity = '1';
    document.getElementById('header-chat-link').style.opacity = '1';
    document.getElementById('footer-chat').style.position = 'relative';
    document.getElementById('footer-chat-link').style.position = 'relative';
    document.getElementById('footer-chat').style.left = '0';
    document.getElementById('footer-chat-link').style.left = '0';
    document.getElementById('footer-chat').style.opacity = '1';
    document.getElementById('footer-chat-link').style.opacity = '1';    
    document.getElementById('contact-page-chat').style.opacity = '1';
    document.getElementById('contact-page-chat').style.visibility = 'visible';
    document.getElementById('contact-page-chat').style.display = 'block';
  });
  olark('api.chat.onOperatorsAway', function() {
    document.getElementById('header-chat').style.position = 'absolute';
    document.getElementById('header-chat-link').style.position = 'absolute';
    document.getElementById('header-chat').style.left = '-10000px';
    document.getElementById('header-chat-link').style.left = '-10000px';
    document.getElementById('header-chat').style.opacity = '0';
    document.getElementById('header-chat-link').style.opacity = '0';
    document.getElementById('footer-chat').style.position = 'absolute';
    document.getElementById('footer-chat-link').style.position = 'absolute';
    document.getElementById('footer-chat').style.left = '-10000px';
    document.getElementById('footer-chat-link').style.left = '-10000px';
    document.getElementById('footer-chat').style.opacity = '0';
    document.getElementById('footer-chat-link').style.opacity = '0';
    document.getElementById('contact-page-chat').style.opacity = '0';
    document.getElementById('contact-page-chat').style.visibility = 'hidden';
    document.getElementById('contact-page-chat').style.display = 'none';
  });
</script>
