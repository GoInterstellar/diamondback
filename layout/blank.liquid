		.contact-section-wrapper input[type="email"]:focus, 
		.contact-section-wrapper input[type="tel"]:focus,
		textarea:focus {
			border: 2px solid #1990c6;
		}
		a:focus,
		input[type^="text"]:focus,
		input[type^="tel"]:focus,
		input[type^="email"]:focus, 
		textarea:focus,
		input[type^="submit"]:focus, 
		input[type^="number"]:focus {
			outline: none;
		}
		body .btn:focus, body .btn:hover {
			background: linear-gradient(#04829f, #06b6e1);
			background-position: 0 0;
			background-repeat: no-repeat;
		}
		.check-mark:focus {
			box-shadow: inset 0 0 5px 4px #04829f;
		}
		.check-mark.selected {
			background: #06b9e4;
			border: 0 solid #06b9e4;
			padding-top: 2px;
		}		
